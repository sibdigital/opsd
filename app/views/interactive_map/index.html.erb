
<style>
  .project-avatar{
    display: none;
  }
  .region{
    opacity: 0.4;
    fill: grey;
  }
  .region:hover
  {
    transition: opacity .3s ease;
    opacity: 1;
  }
  .legend{
    opacity: 0.4;
  }
  .legend:hover
  {
    transition: opacity .3s ease;
    opacity: 1;
  }
  .description
  {
    position: relative;
    vertical-align: top;
    horiz-align: right;
    padding: 20px;
    box-sizing: border-box;
    overflow: scroll;
    width: 90%;
    height: 600px;
    box-shadow: 0 0 32px 10px #CCCCCC;
    display: inline-block;
    margin-left: 5%;
    margin-top: 2%;
    margin-bottom: 2%;
  }
</style>


