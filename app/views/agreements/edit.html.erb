<%#
  needs locals:
  f: labelled form builder
%>
<% breadcrumb_paths(default_breadcrumb, l(:label_edit) + ': ' + l(:label_agreement) + ' №' + @agreement.number_agreement) %>
<% html_title l(:label_agreement) %>
<%= breadcrumb_toolbar l(:label_agreement) %>
<%= labelled_tabular_form_for @agreement, url: project_agreement_path(project_id: @project) do |f| %>
  <%= render partial: 'agreements/form', locals: { f: f } %>
  <%= styled_button_tag l(:button_save), class: '-highlight -with-icon icon-checkmark' %>
  <%= link_to "Печатная форма", {report_id: "agreement"}, class: 'button' %>
  <%= link_to "Приложения 1,2", {report_id: "agreement_pril_1_2"}, class: 'button' %>
<% end %>


